<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EAE7ED"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/header"
        android:layout_width="412dp"
        android:layout_height="107dp"
        android:background="#3006E8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/border"
        android:layout_width="413dp"
        android:layout_height="81dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageButton
            android:id="@+id/Star"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="8dp"
            android:background="#FAF2F2"
            app:layout_constraintStart_toEndOf="@+id/imageButton40"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@mipmap/star_round" />

        <ImageButton
            android:id="@+id/User"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginStart="44dp"
            android:layout_marginTop="8dp"
            android:background="#F8F5F5"
            app:layout_constraintStart_toEndOf="@+id/Star"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@mipmap/user_round" />

        <ImageButton
            android:id="@+id/imageButton39"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:background="#F4F3F3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@mipmap/job_round" />

        <ImageButton
            android:id="@+id/imageButton40"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="8dp"
            android:background="#FAF6F6"
            app:layout_constraintStart_toEndOf="@+id/imageButton39"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@mipmap/company_round" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>